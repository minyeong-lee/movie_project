<?xml version="1.0" encoding="UTF-8"?>
<!-- 테스트 용 쿼리-->
<queries>
    <query id="getAllUsers">
        SELECT
        *
        FROM users
    </query>

    <query id="payMovie">
        INSERT INTO payments (payment_method, payment_price)
        VALUES (?, 19000)
    </query>

    <query id="getAllPayMethods">
        SELECT
        *
        FROM pay_methods
        WHERE user_id = ?
    </query>
</queries>